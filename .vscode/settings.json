{
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk with BibTeX",
            "tools": ["latexmk-bibtex"]
        }
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk-bibtex",
            "command": "latexmk",
